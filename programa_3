# Tablas de multiplicar chuleta 2022 funciona actualizado legal no virus dowload pdf
def buncles():
    nume=(int(input("Tabla del: ")))
    for cont in range(1,10):
        print(cont,"x",nume,"=",cont*nume)
while True:
    buncles()

# BIUCLESSSSS
def buncles():
    nume=(int(input("Número de números: ")))
    nume=nume+1
    suma=0
    for cont in range(1,nume):
        num=(int(input("Numeros: ")))
        suma=suma+num
    print("=",suma)
while True:
    buncles()
